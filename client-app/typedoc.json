{
    "entryPoints": ["src"],
    "entryPointStrategy": "expand",
    "out": "docs",
    "plugin": ["typedoc-plugin-markdown"],
    "exclude": [
      "**/*.spec.ts",
      "**/*.test.ts",
      "**/*.spec.tsx",
      "**/*.test.tsx",
      "**/__tests__/**/*",
      "src/assets/**/*.svg",
      "src/vite-env.d.ts"
    ],
    "tsconfig": "./tsconfig.json",
    "hideGenerator": true,
    "includeVersion": true,
    "excludeExternals": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "readme": "none",
    "name": "Client App Documentation"
  }
  